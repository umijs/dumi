{
  "name": "dumi",
  "version": "2.0.0-beta.0",
  "description": "Framework for developing UI components",
  "keywords": [],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "./bin/dumi.js",
  "files": [
    "bin",
    "dist",
    "compiled",
    "theme-default",
    "theme.d.ts"
  ],
  "scripts": {
    "build": "father build",
    "build:deps": "father prebundle",
    "custom-install": "pnpm i",
    "dev": "father dev",
    "docs:build": "node ./bin/dumi.js build",
    "docs:dev": "node ./bin/dumi.js dev",
    "prepare": "npm run build && node ./bin/dumi.js setup",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepublishOnly": "npm run build",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules pnpm jest"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons-svg": "^4.2.1",
    "@swc/core": "^1.2.224",
    "@types/hast": "^2.3.4",
    "@types/mdast": "^3.0.10",
    "@umijs/bundler-utils": "^4.0.22",
    "@umijs/core": "^4.0.22",
    "dumi-assets-types": "2.0.0-alpha.0",
    "estree-util-to-js": "^1.1.0",
    "estree-util-visit": "^1.2.0",
    "github-slugger": "^1.4.0",
    "hast-util-is-element": "^2.1.2",
    "hast-util-raw": "^7.2.2",
    "hast-util-to-estree": "^2.1.0",
    "hast-util-to-string": "^2.0.0",
    "heti": "^0.9.2",
    "html-to-text": "^8.2.1",
    "js-yaml": "^4.1.0",
    "mdast-util-to-string": "^3.1.0",
    "pluralize": "^8.0.0",
    "prism-react-renderer": "^1.3.5",
    "prismjs": "^1.29.0",
    "raw-loader": "^4.0.2",
    "rc-tabs": "12.1.0-alpha.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-helmet": "^6.1.0",
    "react-intl": "^6.1.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-stringify": "^9.0.3",
    "remark-breaks": "^3.0.2",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "sass": "^1.55.0",
    "umi": "^4.0.22",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.0",
    "url": "^0.11.0",
    "v8-compile-cache": "2.3.0",
    "vfile": "^5.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@jest/types": "^27.0.0",
    "@types/github-slugger": "^1.3.0",
    "@types/jest": "^27.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.6.3",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^18.0.16",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-helmet": "^6.1.5",
    "@umijs/lint": "^4.0.22",
    "@umijs/test": "^4.0.22",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "father": "^4.0.3",
    "husky": "^8.0.1",
    "jest": "^27.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.2.0",
    "stylelint": "^14.9.1",
    "ts-node": "^10.0.0",
    "typescript": "~4.7.4"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "packageManager": "pnpm@7.3.0",
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "Peach <scdzwyxst@gmail.com>"
  ]
}
