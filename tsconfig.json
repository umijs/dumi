{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@@/*": [".dumi/tmp/*"]
    },
    "types": ["vitest/globals"],
    "resolveJsonModule": true
  },
  "include": [".dumi/**/*", ".dumirc.ts", "src/**/*"]
}
